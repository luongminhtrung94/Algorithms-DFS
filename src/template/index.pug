        link(rel="stylesheet" type="text/css" href="./lib/font-awesome/css/font-awesome.min.css")
        
body
    div.form-input(style="max-width:300px;margin-left:auto;margin-right:auto;margin-bottom:30px;margin-top:30px;clear:both")
