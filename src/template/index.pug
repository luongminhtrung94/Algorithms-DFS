            input.form-control.search-value(type="text")
    div#tree.tree
        ul
            li(data-id="1") 
                div.wrap
                    button.minus(type="button")
                        i.fa.fa-minus
                    input(type="text")
                    button.plus(type="button")
                        i.fa.fa-plus
    div(style="clear:both")
    div.container(style="margin-top:30px")
        table.table.table-bordered
            thead
                tr
                    th Số lần lặp
                    th X
                    th Open
                    th Close
            tbody
    div.container
        div.result
                

    
