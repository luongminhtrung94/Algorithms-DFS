        title asasd
        meta(charset="UTF-8")
        meta(content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport')
        //- bootstrap
        link(rel="stylesheet" type="text/css"  href="./lib/bootstrap/css/bootstrap.min.css")
